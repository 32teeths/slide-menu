{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "ordered-imports": [
      false
    ],
    "space-before-function-paren": [
      false
    ]
  },
  "rulesDirectory": []
}
